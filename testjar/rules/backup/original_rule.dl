ArrayIndexPointsTo(?immHctx, ?val, ?immHctx, ?mockArray),
Value_isMock(?mockArray), isValue(?mockArray), Value_Type(?mockArray,  ?varArgsType), Value_DeclaringType(?mockArray, "java.lang.Object"),
